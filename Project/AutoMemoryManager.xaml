    <ui:InvokeCode ContinueOnError="{x:Null}" sap2010:Annotation.AnnotationText="Enter directories as an array of directories and maxDataLife which is the number of days  a file or folder not to exceed in the computer" Code="Try&#xA;Dim todaysDate As DateTime = System.DateTime.Now&#xA;If directories IsNot Nothing AndAlso directories.Length &gt; 0&#xA;&#x9;For Each directoryPath As String In directories&#xA;&#x9;&#x9;If System.IO.Directory.Exists(directoryPath) Then&#xA;&#x9;&#x9;&#x9;'Checks if subfolder in the designated director is too old, if too old deletes them&#xA;&#x9;&#x9;&#x9;Dim innerfolders As String() = System.IO.Directory.GetDirectories(directoryPath)&#xA;&#x9;&#x9;&#x9;&#x9;If  innerfolders IsNot Nothing AndAlso innerfolders.Length &gt; 0&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;For Each innerfolder As String In innerfolders&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Dim createDate As DateTime = System.IO.Directory.GetCreationTime(innerfolder)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Dim existence As Integer = todaysDate.Subtract(createDate).Days&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;If existence &gt;= maxDataLife&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'Delete inner folder with recursive because we what to delete inner folder and its contents&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Console.WriteLine(&quot;The folder &quot;+Path.GetFileNameWithoutExtension(innerfolder)+&quot; is has been on this drive for more than &quot;+maxDataLife.ToString+&quot;days and would be deleted &quot;)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;System.IO.Directory.Delete(innerfolder,True)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Console.WriteLine(&quot;The folder &quot;+Path.GetFileNameWithoutExtension(innerfolder)+&quot; and its contents have been deleted &quot;)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Else&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'do nothing&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;End If&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Next&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;End If&#xA;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;'Checks if a file  in the designated directory is too old, if too old deletes them&#xA;&#x9;&#x9;&#x9;Dim innerfiles As String() = System.IO.Directory.GetFiles(directoryPath)&#xA;&#x9;&#x9;&#x9;&#x9;If  innerfiles IsNot Nothing AndAlso innerfiles.Length &gt; 0&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;For Each innerfile As String In innerfiles&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Dim createDate As DateTime = System.IO.File.GetCreationTime(innerfile)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Dim existence As Integer = todaysDate.Subtract(createDate).Days&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;If existence &gt;= maxDataLife&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'Delete inner file&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Console.WriteLine(&quot;The file &quot;+Path.GetFileNameWithoutExtension(innerfile)+&quot; is has been on this drive for more than &quot;+maxDataLife.ToString+&quot;days and would be deleted &quot;)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;System.IO.File.Delete(innerfile)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Console.WriteLine(&quot;The file &quot;+Path.GetFileNameWithoutExtension(innerfile)+&quot; and its contents have been deleted &quot;)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Else&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;'do nothing&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;End If&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Next&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;End If&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;End If&#xA;&#x9;Next&#xA;End If&#xA;Catch ex As Exception&#xA;&#x9;&#xA;&#x9;errorMessage = ex.Message&#xA;&#x9;&#xA;End Try&#xA;&#xA;'Solution by Master Of Logic Academy" DisplayName="AutoMemoryManager" sap:VirtualizedContainerService.HintSize="334,226" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
